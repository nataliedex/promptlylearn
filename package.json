{
  "name": "promptlylearn",
  "version": "1.0.0",
  "description": "This is the start of Promptly an innovative way to learn and to test",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/cli/runAssignment.ts",
    "dev:api": "nodemon --exec ts-node src/api/index.ts",
    "dev:web": "cd web && npm run dev",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "build": "tsc",
    "build:web": "cd web && npm run build",
    "start": "node dist/index.js",
    "start:api": "node dist/api/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "Natalie Blumen",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^6.15.0",
    "uuid": "^13.0.0"
  }
}
